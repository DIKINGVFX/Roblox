game:GetService("StarterGui"):SetCore("SendNotification",{Title="DMS Gui",Text="By DIKINGVFX", Duration=5})

local library = loadstring(game:HttpGet(('https://pastebin.com/raw/FsJak6AT')))()
local w = library:CreateWindow("Dogecoin Mining Simulator")
local a = w:CreateFolder("Main Land")
local b = w:CreateFolder("Desert Land")
local c = w:CreateFolder("Ice Land")
local d = w:CreateFolder("Lava Land")
local e = w:CreateFolder("Obsidian Land")

a:Button("Teleport To",function(bool)
    for i = 1,100 do
     TpTo(game:GetService("Workspace").Eggs.Basic.UIanchor.CFrame)
    end
end)


a:Toggle("AutoClick",function(bool)
    getgenv().auto1 = bool
end)

a:Toggle("AutoBuyEgg",function(bool)
    getgenv().auto2 = bool
end)

a:Toggle("AutoBuyPremiumEgg",function(bool)
    getgenv().auto3 = bool
end)

a:Button("Unlock All Rebirth",function(bool)
    for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.Main.MainFrame.RebithsShop.ScrollingFrame:GetChildren()) do
        if v.Name == "UIGridLayout" then
            continue
        end
        print(v)
        v.Visible = true
    end
end)


b:Button("Teleport To",function(bool)
    TpTo(game:GetService("Workspace").Portals.Desert.Teleport.CFrame)
end)

b:Toggle("AutoBuyEgg",function(bool)
    getgenv().auto4 = bool
end)

c:Button("Teleport To",function(bool)
    TpTo(game:GetService("Workspace").Portals["Snow Land"].Teleport.CFrame)
end)

c:Toggle("AutoBuyEgg",function(bool)
    _getgenv()G.auto5 = bool
end)

d:Button("Teleport To",function(bool)
    TpTo(game:GetService("Workspace").Portals["Lava Land"].Teleport.CFrame)
end)

d:Toggle("AutoBuyEgg",function(bool)
    getgenv().auto6 = bool
end)

e:Button("Teleport To",function(bool)
    TpTo(game:GetService("Workspace").Portals["Obsidian Land"].Teleport.CFrame)
end)

e:Toggle("AutoBuyEgg",function(bool)
    getgenv().auto7 = bool
end)


function GetPos()
    local plyr = game.Players.LocalPlayer;
    if plyr.Character then
        return plyr.Character.HumanoidRootPart.CFrame;
    end
    return false;
end

function TpTo(playerCFRame)
    local plyr = game.Players.LocalPlayer;
    if plyr.Character then
        plyr.Character.HumanoidRootPart.CFrame = playerCFRame;
    end
end

--Function

while wait() do
      
    if auto1 == true then
        game.Workspace.Events.AddClick:FireServer()
    end
    
    if auto2 == true then
        local args = {[1] = "Basic",[2] = "Single"}    
        game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
    end
    
    if auto3 == true then
        local args = {[1] = "ea",[2] = "Single"}    
        game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
    end
    
    if auto4 == true then
        local args = {[1] = "DesertEgg",[2] = "Single"} 
        game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
        
    end

    if auto5 == true then
        local args = {[1] = "IceEgg",[2] = "Single"}    
        game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
    end

    if auto6 == true then
        local args = {[1] = "ForestEgg",[2] = "Single"}    
        game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
    end

    if auto7 == true then
        local args = {[1] = "ObsidianEgg",[2] = "Single"}    
        game:GetService("ReplicatedStorage").RemoteEvents.EggOpened:InvokeServer(unpack(args))
    end
    
end
